<%= form_for @post do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<%= f.label :video_url %>
	<%= f.text_field :video_url %>
	<%= f.label :content %>
	<%= f.text_area :content %>


	<%= f.fields_for :tags do |t| %>
		<%= t.label :tag %>
		<%= t.text_field :name %>
<% end %>	
	<%= f.submit "Create Post" %>
<% end %>	
