<h1> Users </h1>
<hr>


<div class="user_list" >
	

	<% @users.each do |user| %>
	 <div class="user_profile_name">
      <%= link_to user.name, user %>   
    </div>  
	<div class="user_image">
		<% if user.image_url %>    
  		<%= image_tag user.image_url %>
		<% else %>  
  		<%= image_tag asset_path 'default_profile.png' %>
		<% end %>
	</div>
  
   
	<% end %>
</div>