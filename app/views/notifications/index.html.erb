<h1> News Feed </h1>

<hr>
<div class="newsfeed_container">
	<% if @activity %>
	  <% @activities.each do |activity| %>
	    <% if activity.key == "post.create" and activity.trackable %>
	      <div class="activity">
	        <%= render_activity(activity) %>
	      </div>  
	    <% end %> 
	  <% end %>
	<% end %>
</div>
